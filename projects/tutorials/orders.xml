          <Condition/>
        </Assignment>
        <Assignment>
          <AssignName>order_date</AssignName>
          <AssignNameNew/>
          <Expression>substr({order_date},0,10)</Expression>
          <Condition/>
        </Assignment>
        <Assignment>
          <AssignName>linevalue</AssignName>
          <AssignNameNew/>
          <Expression>{price} * {quantity}</Expression>
          <Condition/>
        </Assignment>
        <Assignment>
          <AssignName>linediscount</AssignName>
          <AssignNameNew/>
          <Expression>{discount} * {quantity}</Expression>
          <Condition/>
        </Assignment>
        <Assignment>
          <AssignName>linevalue</AssignName>
          <AssignNameNew/>
          <Expression>({linevalue} - {linediscount})</Expression>
          <Condition/>
        </Assignment>
        <Assignment>
          <AssignName>ordervalue</AssignName>
          <AssignNameNew/>
          <Expression>sum({linevalue},{order_id})</Expression>
          <Condition/>
        </Assignment>
        <Assignment>
          <AssignName>ordervalue</AssignName>
          <AssignNameNew/>
          <Expression>"$".number_format({ordervalue},2)</Expression>
          <Condition/>
        </Assignment>
        <Assignment>
